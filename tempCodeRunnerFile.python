import pygame
import sys

pygame.init()

#screen setup
screen_width, screen_height = 1080, 720
screen = pygame.display.set_mode((screen_width, screen_height))
pygame.display.set_caption("Run Quest")

#gameloop

running=True
while running:
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            running = False




